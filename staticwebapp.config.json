{
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "routes": [
    {
      "route": "/login",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/assets/*",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/favicon.ico",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/*",
      "allowedRoles": [
        "authenticated"
      ]
    }
  ],
  "responseOverrides": {
    "401": {
      "statusCode": 302,
      "redirect": "/login"
    }
  }
}